<%= form_for tour_request do |f| %>

  <%= render partial: "errors", locals: { tour_request: tour_request } %>

  <%= f.label :first_name %>
  <%= f.text_field :first_name %>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %>

  <%= f.label :phone %>
  <%= f.text_field :phone %>

  <%= f.submit "Submit" %>
<% end %>
